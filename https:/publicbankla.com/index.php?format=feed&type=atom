	<entry>
		<title>Public Bank LA Letter</title>
		<link rel="alternate" type="text/html" href="https://publicbankla.com/index.php/about/resources/37-public-bank-la-letter"/>
		<published>2017-11-29T20:26:00+00:00</published>
		<updated>2017-11-29T20:26:00+00:00</updated>
		<id>https://publicbankla.com/index.php/about/resources/37-public-bank-la-letter</id>
		<author>
			<name>Public Bank LA</name>
		</author>
